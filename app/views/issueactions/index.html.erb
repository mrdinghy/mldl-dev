<p id="notice"><%= notice %></p>

<h1>Listing Issueactions</h1>

<table>
  <thead>
    <tr>
      <th>Actiontype</th>
      <th>Meeting</th>
      <th>Organization</th>
      <th>Issue</th>
      <th>Actionbody</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @issueactions.each do |issueaction| %>
      <tr>
        <td><%= issueaction.actiontype_id %></td>
        <td><%= issueaction.meeting_id %></td>
        <td><%= issueaction.organization_id %></td>
        <td><%= issueaction.issue_id %></td>
        <td><%= issueaction.actionbody %></td>
        <td><%= link_to 'Show', issueaction %></td>
        <td><%= link_to 'Edit', edit_issueaction_path(issueaction) %></td>
        <td><%= link_to 'Destroy', issueaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Issueaction', new_issueaction_path %>
