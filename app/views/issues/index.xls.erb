

<% wb = xlsx_package.workbook
wb.add_worksheet(name: "Issues") do |sheet|
@issues.each do |issue|sheet.add_row [
                      issue.issuecode,
                      issue.structure.name,
                      issue.issue.name,
                      issue.category.name,
                      issue.district.name,
                      issue.community,
                      issue.status_humanize,
                      issue.resolution_date,
                      issue.raised_by.fullname,
                      issue.raised_on,
                      issue.created_at]
end
end
  %>

