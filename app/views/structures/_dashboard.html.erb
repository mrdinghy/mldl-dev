
<%= debug @thisqtr %>


<h2><%= @thisyear %>, <%#= @structure.project.qtrname(@thisqtr) %></h2>

<div class="content-wrapper widerbox">

  <div class="row">


    <div class="col-sm-4">
      <div class="panel panel-default">

        <div class="panel-body">
          <table  class="table table-striped table-hover">


            <tr><td>Open Issues Beginning Quarter</td><td><%= @structure.project.beginissues(@d2,@structure.id,0,0).count %></td></tr>
            <tr><td>News Issues</td><td><%= @structure.project.quarter_issue_total(@d2,@structure.id,0,0) %></td></tr>
            <tr><td>Resolved</td><td><%= @structure.project.quarter_issue_resolved(@d2,@structure.id,0,0) %></td></tr>
            <tr><td>Meeting Agendas</td><td><%= @structure.project.get_agendas(@d2,@structure.id,0,0).count %></td></tr>
            <tr><td>Agenda: Resolved</td><td><%= @structure.project.agenda_results(@d2,@structure.id,Result::RESOLVED,0,0).count %></td></tr>
            <tr><td>Agenda: Mediation</td><td><%= @structure.project.agenda_results(@d2,@structure.id,Result::MEDIATION,0,0).count %></td></tr>
            <tr><td>Agenda: Cancelled</td><td><%= @structure.project.agenda_results(@d2,@structure.id,Result::CANCELLED,0,0).count %></td></tr>
            <tr><td>Agenda: Referred</td><td><%= @structure.project.agenda_results(@d2,@structure.id,Result::ESCALATED,0,0).count %></td></tr>
            <tr><td>Agenda: Ongoing</td><td><%= @structure.project.agenda_results(@d2,@structure.id,Result::ONGOING,0,0).count %></td></tr>
            <tr><td>Mediations Started</td><td><%= @structure.project.mediation_starts(@d2,@structure.id,0,0).count %></td></tr>
            <tr><td>Mediations Ended</td><td><%= @structure.project.mediation_ends(@d2,@structure.id,0,0).count %></td></tr>
            <tr><td>Mediation Result: Resolved</td><td><%= @structure.project.mediation_results(@d2,@structure.id,Result::RESOLVED,0,0).count %></td></tr>
            <tr><td>Mediation Result: Cancelled</td><td><%= @structure.project.mediation_results(@d2,@structure.id,Result::CANCELLED,0,0).count %></td></tr>
            <tr><td>Mediation Result: Referred</td><td><%= @structure.project.mediation_results(@d2,@structure.id,Result::ESCALATED,0,0).count %></td></tr>
            <tr><td>Mediation Result: Ongoing</td><td><%= @structure.project.mediation_results(@d2,@structure.id,Result::ONGOING,0,0).count %></td></tr>


          </table>
        </div>
      </div>
    </div>





    <div class="col-sm-4">
      <div class="panel panel-default">

        <div class="panel-body">
          <table  class="table table-striped table-hover">
            <thead>
            <tr>
              <td>Category</td>
              <td>Begin Quater Open</td>
              <td>New Issues</td>
              <td>Resolved</td>
            </tr>

            </thead>
            <% Category.all.each do |c| %>


                <tr>
                  <td><%= c.name %></td>
                  <td><%= @structure.project.beginissues(@d2,@structure.id,c.id,0).count %></td>
                  <td><%= @structure.project.quarter_issue_total(@d2,@structure.id,c.id,0) %></td>
                  <td><%= @structure.project.quarter_issue_resolved(@d2,@structure.id,c.id,0) %></td>
                </tr>


            <% end %>


          </table>
        </div>
      </div>
    </div>






    <div class="col-sm-4">
      <div class="panel panel-default">

        <div class="panel-body">
          <table  class="table table-striped table-hover">
            <thead>
            <tr>
              <td>District</td>
              <td>Begin Quater Open</td>
              <td>New Issues</td>
              <td>Resolved</td>
            </tr>

            </thead>
            <% District.all.each do |d| %>
                <% dissues = Issue.where(district_id: d.id) %>
                <% if dissues.count != 0 %>
                    <tr>
                      <td><%= d.name %></td>
                      <td><%= @structure.project.beginissues(@d2,@structure.id,0,d.id).count %></td>
                      <td><%= @structure.project.quarter_issue_total(@d2,@structure.id,0,d.id) %></td>
                      <td><%= @structure.project.quarter_issue_resolved(@d2,@structure.id,0,d.id) %></td>
                    </tr>
                <% end %>

            <% end %>


          </table>
        </div>
      </div>
    </div>









  </div>
</div>










































