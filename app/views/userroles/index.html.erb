<p id="notice"><%= notice %></p>

<h1>Listing Userroles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Structure</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @userroles.each do |userrole| %>
      <tr>
        <td><%= userrole.user_id %></td>
        <td><%= userrole.structure_id %></td>
        <td><%= userrole.role_id %></td>
        <td><%= link_to 'Show', userrole %></td>
        <td><%= link_to 'Edit', edit_userrole_path(userrole) %></td>
        <td><%= link_to 'Destroy', userrole, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Userrole', new_userrole_path %>
